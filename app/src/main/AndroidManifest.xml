<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="braess.constantin.wgcompanion">

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/wg_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/wg_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="HardcodedDebugMode"
        tools:targetApi="donut">
        <activity android:name="braess.constantin.wgterminal.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="braess.constantin.wgterminal.todo"
            android:label="@string/title_activity_todo"/>
    </application>

</manifest>